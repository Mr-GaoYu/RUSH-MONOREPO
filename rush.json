{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.75.0",
  "pnpmVersion": "7.1.9",
  "pnpmOptions": {
    "useWorkspaces": true
  },
  "nodeSupportedVersionRange": ">=12.17.0 <17.0.0",
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 3,
  "ensureConsistentVersions": true,
  "repository": {
    "url": "https://github.com/Mr-GaoYu/RUSH-MONOREPO",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },
  "approvedPackagesPolicy": {
    "reviewCategories": [
      "frontend",
      "backend",
      "common",
      "tools",
      "internal",
      "extensions"
    ],
    "ignoredNpmScopes": [
      "@types"
    ]
  },
  "gitPolicy": {
    "allowedEmailRegExps": [
      "[^@]+@163\\.com"
    ],
    "sampleEmail": "mrexample@163.com"
  },
  "projects": [
    {
      "packageName": "@rom/build-tools",
      "projectFolder": "tools/build",
      "reviewCategory": "tools",
      "shouldPublish": true,
      "versionPolicyName": "prerelease-monorepo-lockStep"
    }
  ]
}